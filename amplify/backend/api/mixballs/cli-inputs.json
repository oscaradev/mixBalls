{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "mixballs",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "AMAZON_COGNITO_USER_POOLS",
      "cognitoUserPoolId": "authmixBalls"
    },
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "User"
        }
      ]
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      }
    ]
  }
}